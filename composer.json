{
  "name": "aaxis/webpack-build",
  "type": "symfony-bundle",
  "description": "AAXIS Webpack Bundle",
  "homepage": "https://github.com/tabhan/webpack-build.git",
  "keywords": ["AAXIS", "Webpack"],
  "license": "OSL-3.0",
  "require": {
    "oro/platform": "4.0.*"
  },
  "autoload": {
    "psr-4": { "AAXIS\\Bundle\\WebpackBundle\\": "" },
    "exclude-from-classmap": ["/Tests/"]
  },
  "repositories": {
    "oro": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.orocrm.com"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0-dev"
    }
  }
}
